{
    "manifest_version": 2,
    "name": "Dracula News",
    "version": "1.0",
    "description": "Adds Dracula colour scheme to Hacker News and improves comment UI",
    "icons": {
        "48": "icons/skull_bold.svg"
    },
    "content_scripts": [
        {
            "matches": [
                "*://news.ycombinator.com/*"
            ],
            "js": [
                "draculaNews.js"
            ]
        }
    ]
}